badcoding(x1, x2, x3, x4, x5, x6, x7, x8, x9, x10; y1, y2);

proc badcoding(in x1, x2, x3, x4, x5, x6, x7, x8, x9, x10; out y1, y2) {
  while (x1) 
  {
    while (x2) 
    {
      while (x3) 
      {
        while (x4) 
        {
          while (x5) 
          {
            while (x6) 
            {
              while (x7) 
              {
                while (x8) 
                {
                  while (x9) 
                  {
                    while (x10) 
                    {
                      y2 = y1 + 1;
                      y1 = y2 + 2;
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}